{% extends 'base.html.twig' %}

{% block title %}TypeInstrument index{% endblock %}

{% block body %}
    <h1>TypeInstrument index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Libelle</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_instrument in type_instruments %}
            <tr>
                <td>{{ type_instrument.id }}</td>
                <td>{{ type_instrument.libelle }}</td>
                <td>
                    <a href="{{ path('app_type_instrument_show', {'id': type_instrument.id}) }}">show</a>
                    <a href="{{ path('app_type_instrument_edit', {'id': type_instrument.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_instrument_new') }}">Create new</a>
{% endblock %}
